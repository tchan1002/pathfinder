    "jsdom": "^26.1.0",
    "next": "15.5.2",
    "openai": "^5.16.0",
    "pg": "^8.16.3",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "robots-parser": "^3.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "playwright": "^1.55.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
